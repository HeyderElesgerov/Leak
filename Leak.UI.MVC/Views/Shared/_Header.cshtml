<script>
    let blogsContainer = document.getElementById('blogsContainer');

    fetch('/api/blogs')
        .then(resp => resp.json())
        .then(blogs => {
            for (let i = 0; i < blogs.length; i++) {
                addBlogView(blogs[i])
            }
        });

    function addBlogView(blog) {
        blogsContainer.innerHTML += `<li><a href="/Blog/${blog.url}">${blog.title}</a></li>`;
    }
</script>